{
  "name": "nodejs-typescript-eslint-template",
  "version": "1.0.0",
  "description": "Robust TypeScript Template with eslint for Node.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --import tsx/esm src/index.ts",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "lint": "tsc --noEmit && eslint --cache --fix",
    "prettier": "prettier --write",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "author": "Kamesuta",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/node": "^20.13.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.13"
  },
  "dependencies": {
    "log4js": "^6.9.1",
    "toml": "^3.0.0"
  }
}
